<%= form_for @baby do |f| %>
  <p><%= f.label(:first_name) %>
  <%= f.text_field(:first_name) %></p>
  <p><%= f.label(:last_name) %>
  <%= f.text_field(:last_name) %></p>
  <p><%= f.label(:weight) %>
  <%= f.text_field(:weight) %></p>
  <p><%= f.label(:birth_date) %>
  <%= f.text_field(:birth_date) %></p>
  <p><%= submit_tag("Create Baby") %></p>
  <p><%= submit_tag("Update Baby") %></p>
<% end %>
